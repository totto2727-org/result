{
  "name": "@totto2727/result",
  "author": "totto2727 <kaihatu.totto2727@gmail.com>",
  "description": "Rust-inspired Result-type package",
  "homepage": "https://github.com/totto2727-org/result#readme",
  "bugs": {
    "url": "https://github.com/totto2727-org/result/issues"
  },
  "keywords": [
    "result",
    "utils",
    "fp",
    "functional",
    "programming",
    "typescript"
  ],
  "license": "MIT",
  "version": "0.6.0",
  "scripts": {
    "deploy": "",
    "build": "bun run build:transpile && bun run build:decolation",
    "build:transpile": "bun build.ts",
    "build:decolation": "tsc --project tsconfig.build.json",
    "check": "bun run check:type && bun run check:static",
    "check:type": "tsc --noEmit",
    "check:static": "biome check *.ts *.json",
    "fix": "biome check --apply-unsafe *.ts *.json",
    "test": "bun test",
    "precommit": "bun run check:type && bun run fix && bun run test && bun run build"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/require/index.cjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/import/index.mjs",
      "require": "./dist/require/index.cjs"
    },
    "./eager": {
      "types": "./dist/types/eager.d.ts",
      "import": "./dist/import/eager.mjs",
      "require": "./dist/require/eager.cjs"
    },
    "./lazy": {
      "types": "./dist/types/lazy.d.ts",
      "import": "./dist/import/lazy.mjs",
      "require": "./dist/require/lazy.cjs"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.1.2",
    "@changesets/cli": "^2.26.2",
    "bun-types": "^1.0.1",
    "esbuild": "^0.19.8",
    "typescript": "5.2.2"
  }
}
